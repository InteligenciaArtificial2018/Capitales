        app:layout_constraintTop_toBottomOf="@+id/tvSeisnivel2" />

    <TextView
        android:id="@+id/tv3Nivel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginEnd="90dp"
        android:text="@string/tv3Seis2"
        app:layout_constraintEnd_toStartOf="@+id/tv4Nivel2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn1Seis2" />

    <TextView
        android:id="@+id/tv4Nivel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="80dp"
        android:text="@string/tv4Seis2"
        app:layout_constraintEnd_toEndOf="parent"
